class Solution {
public:
    vector<int> sortArrayByParity(vector<int>& A) {
       vector<int>res;
      int x = 0;
        int i;
        for(i =0;i<A.size();i++){
            if(A[i]%2==0)
                res.push_back(A[i]);
        }
        for(auto x:A)
        {
            if(x%2!=0)
                res.push_back(x);
        }
        return res;
    }
};

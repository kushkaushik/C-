class Solution {
public:
    int firstMissingPositive(vector<int>& nums) {
       bool flag = false;
        int l = nums.size();
        int temp[nums.size()+1];
        fill(temp,temp+l+1,false);
        for(int i=0;i<l;i++){
            if(nums[i]>0 && nums[i]<=l)
                temp[nums[i]] = true;
        }
        for(int i=1;i<=l;i++){
            if(temp[i] == false)
                return i;
        }
        return l+1;
    
        }
};
